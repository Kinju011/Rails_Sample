<h1>Books#index</h1>
<p>Find me in app/views/books/index.html.erb</p>
<%= text_field_tag :search, "" , class:"search_book" %>
<tbody class="book_tbody_content">
	<%= render 'index_content' %>
</tbody>

<script type="text/javascript">	
    $(".search_book").keyup(function(){
    	debugger
    	var src = $(".search_book").val();
        $.ajax({
        	type:'GET',
          url:"/books",
          data: JSON.stringify(src),
          dataType: "json",
          contentType: "application/json; charset=utf-8",
          success: function(data){
          	alert(data.message);
          },
          error: function(data){
          	alert(data.msg);
          }
        });
    });
    
</script>